<mvc:View xmlns:form="sap.fe.macros.form" xmlns="sap.m" controllerName="djp.clouddna.demo.controller.Additional" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">

    <Page id="page2" title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack" backgroundDesign="Transparent" >

        <customHeader>
            <Bar id="_IDGenBar1">
                <contentLeft>
                    <Button id="_IDGenButton1" icon="sap-icon://nav-back" press="onNavBack"/>
                </contentLeft>
                <contentMiddle>
                    <Title id="_IDGenTitle1" text="Customer Details"/>
                    <SegmentedButton id="_IDGenSegmentedButton">
                        <items>
                            <SegmentedButtonItem id="_IDGenSegmentedButtonItem" text="Info"/>
                            <SegmentedButtonItem id="_IDGenSegmentedButtonItem1" text="Items"/>
                            <SegmentedButtonItem id="_IDGenSegmentedButtonItem2" text="History"/>
                        </items>
                    </SegmentedButton>
                </contentMiddle>
                <contentRight>
                    <SearchField id="_IDGenSearchField" width="100%" placeholder="Search Products"/>
                    <Button id="_IDGenButton2" text="Create" press="onCreate"/>
                    <Button id="_IDGenButton3" icon="sap-icon://action" press="onMore"/>
                </contentRight>
            </Bar>
        </customHeader>

        <content>
            <VBox id="_IDGenVBoxD" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle25" text="SAP.M.TEXT" class="titleBg" width="1230px" />
                <Text id="_IDGenText" class="sapUiSmallMarginBegin" text="Hello World" maxLines="1" wrapping="true"/>
            </VBox>
            <VBox id="_IDGenVBox" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle24" text="SAP.M.TITLE" class="titleBg" width="1230px" />
                <Title id="_IDGenTitle2" class="sapUiSmallMarginBegin" text="This is a title" titleStyle="H1"/>
            </VBox>
            <VBox id="_IDGenVBox1" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle23" text="SAP.M.INPUT" class="titleBg" width="1230px" />
                <Input id="_IDGenInput" class="sapUiSmallMarginBegin" placeholder="Provide something" description="Unit" width="300px"/>
            </VBox>
            <VBox id="_IDGenVBox2" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle22" text="SAP.M.SUGGESTIONITEMS" class="titleBg" width="1230px" />
                <Input id="_IDGenInput1" class="sapUiSmallMarginBegin sapUiSmallMarginBegin" showSuggestion="true" showClearIcon="true" width="300px">
                    <suggestionItems>
                        <core:Item id="_IDGenItem1" key="1" text="first"/>
                        <core:Item id="_IDGenItem2" key="2" text="second"/>
                        <core:Item id="_IDGenItem3" key="3" text="third"/>
                    </suggestionItems>
                </Input>
            </VBox>
            <VBox id="_IDGenVBox3" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle21" text="SAP.M.STEPINPUT" class="titleBg" width="1230px" />
                <StepInput id="_IDGenStepInput" class="sapUiSmallMarginBegin" value="10" step="10" max="100" min="0" width="300px" />
            </VBox>
            <VBox id="_IDGenVBox4" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle20" text="SAP.M.MASKINPUT" class="titleBg" width="1230px" />
                <MaskInput id="_IDGenMaskInput" class="sapUiSmallMarginBegin" mask="+81 9999 9999999999" width="350px"/>
            </VBox>
            <VBox id="_IDGenVBox5" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle19" text="SAP.M.TEXTAREA" class="titleBg" width="1230px" />
                <TextArea id="_IDGenTextArea" class="sapUiSmallMarginBegin" value="I'm a really long text" rows="6"/>
            </VBox>
            <VBox id="_IDGenVBox6" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle18" text="SAP.M.DATEPICKER" class="titleBg" width="1230px" />
                <DatePicker class="sapUiSmallMarginBegin" id="datePicker" width="350px" />
            </VBox>
            <VBox id="_IDGenVBox7" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle17" text="SAP.M.CHECKBOX" class="titleBg" width="1230px" />
                <CheckBox id="_IDGenCheckBox" class="sapUiSmallMarginBegin" text="Do you like this book" selected="true" />
            </VBox>
            <VBox id="_IDGenVBox8" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle16" text="SAP.M.SWITCH" class="titleBg" width="1230px" />
                <Label id="_IDGenLabel" text="Do you like this book?" width="350px"/>
                <Switch id="_IDGenSwitch" customTextOn="Sure" customTextOff="Meh" state="true" />
            </VBox>
            <VBox id="_IDGenVBox9" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle15" text="SAP.M.RADIOBUTTON/GROUP" class="titleBg" width="1230px" />
                <RadioButtonGroup id="_IDGenRadioButtonGroup" selectedIndex="0" >
                    <RadioButton id="_IDGenRadioButton" text="I like it"/>
                    <RadioButton id="_IDGenRadioButton1" text="I like it very much"/>
                    <RadioButton id="_IDGenRadioButton2" text="I like something else"/>
                </RadioButtonGroup>
            </VBox>
            <VBox id="_IDGenVBox10" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle14" text="SAP.M.RANGESLIDER" class="titleBg" width="1230px" />
                <RangeSlider id="_IDGenRangeSlider" min="0" max="200" range="0,150" showAdvancedTooltip="true" enableTickmarks="true" width="350px" />
            </VBox>
            <VBox id="_IDGenVBox11" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle13" text="SAP.M.SELECT" class="titleBg" width="1230px" />
                <Label id="_IDGenLabel1" text="My favorite SAP Technology" />
                <Select id="_IDGenSelect" forceSelection="false" selectedKey="" >
                    <items>
                        <core:Item id="_IDGenItem4" key="rap" text="ABAP RESTful Application Programming Model" />
                        <core:Item id="_IDGenItem5" key="segw" text="SAP Gateway Service" />
                        <core:Item id="_IDGenItem6" key="cap" text="Cloud Application Programming Model" />
                    </items>
                </Select>
            </VBox>
            <VBox id="_IDGenVBox12" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle12" text="SAP.M.COMBOBOX" class="titleBg" width="1230px" />
                <Label id="_IDGenLabel2" text="My favourite SAP Technology" />
                <ComboBox id="_IDGenComboBox" >
                    <items>
                        <core:Item id="_IDGenItem7" key="rap" text="ABAP RESTful Application Programming Model"/>
                        <core:Item id="_IDGenItem8" key="segw" text="SAP Gateway Service"/>
                        <core:Item id="_IDGenItem9" key="cap" text="Cloud Application Programming Model"/>
                    </items>
                </ComboBox>
            </VBox>
            <VBox id="_IDGenVBox13" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle3" text="SAP.M.BUTTON" class="titleBg" width="1230px" />
                <Button id="_IDGenButton5" text="I'm a default button" />
                <Button id="_IDGenButton6" text="Accept" type="Accept" />
                <Button id="_IDGenButton7" text="Reject" type="Reject" />
                <Button id="_IDGenButton8" text="I'm transparent" type="Transparent" />
                <Button id="_IDGenButton9" type="Back" />
                <Button id="_IDGenButton10" text="I'm important" type="Emphasized"/>
                <Button id="_IDGenButton11" text="I have an icon" icon="sap-icon://education"/>
            </VBox>
            <VBox id="_IDGenVBox14" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle4" text="SAP.M.MenuButton" class="titleBg" width="1230px" />
                <MenuButton id="_IDGenMenuButton" text="Im a MenuButton" menuPosition="BeginBottom" buttonMode="Split" >
                    <!-- <menu> -->
                        <Menu id="_IDGenMenu" >
                            <MenuItem id="_IDGenMenuItem1" text="I'm a MenuItem" icon="sap-icon://edit"/>
                            <MenuItem id="_IDGenMenuItem2" text="I'm a SubMenu">
                                <items>
                                    <MenuItem id="_IDGenMenuItem3" text="I'm a MenuItem"/>
                                    <MenuItem id="_IDGenMenuItem4" text="I'm a MenuItem too"/>
                                </items>
                            </MenuItem>
                        </Menu>
                    <!-- </menu> -->
                </MenuButton>
            </VBox>
            <VBox id="_IDGenVBox15" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle5" text="SAP.M.SegmentedButton" class="titleBg" width="1230px" />
                <SegmentedButton id="_IDGenSegmentedButton1" selectedKey="second" >
                    <items>
                        <SegmentedButtonItem id="_IDGenSegmentedButtonItem4" key="first" text="I'm first"/>
                        <SegmentedButtonItem id="_IDGenSegmentedButtonItem5" key="second" text="I'm second"/>
                        <SegmentedButtonItem id="_IDGenSegmentedButtonItem6" key="third" text="I'm third"/>
                    </items>
                </SegmentedButton>
            </VBox>
            <VBox id="_IDGenVBox16" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle6" text="SAP.M.LINK" class="titleBg" width="1230px" />
                <Link id="_IDGenLink" text="I open SAPUI5" href="https://ui5.sap.com" target="_blank"/>
            </VBox>
            <VBox id="_IDGenVBox17" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle7" text="SAP.UI.CORE.ICON" class="titleBg" width="1230px" />
                <core:Icon id="_IDGenIcon" src="sap-icon://course-book" color="blue" activeBackgroundColor="yellow" activeColor="magenta" alt="Alternative text"
                            hoverBackgroundColor="brown" hoverColor="purple" />
            </VBox>
            <VBox id="_IDGenVBox18" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle8" text="SAP.UI.LAYOUT.FORM with ColumnLayout" class="titleBg" width="1230px" />
                <f:Form id="_IDGenForm" editable="true" >
                    <f:title>
                        <core:Title id="_IDGenTitle30" text="Book Data"></core:Title>
                    </f:title>
                    <f:layout>
                        <f:ColumnLayout id="_IDGenColumnLayout" columnsM="1" columnsL="2" columnsXL="2"/>
                    </f:layout>
                    <f:formContainers>
                        <f:FormContainer id="_IDGenFormContainer1">
                            <f:formElements>
                                <f:FormElement id="_IDGenFormElement" label="Book name">
                                    <f:fields>
                                        <Input id="_IDGenInput2" placeholder="Book name"/>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement id="_IDGenFormElement1" label="ISBN">
                                    <f:fields>
                                        <Input id="_IDGenInput3" placeholder="ISBN"/>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement id="_IDGenFormElement2" label="Author">
                                    <f:fields>
                                        <Input id="_IDGenInput4" placeholder="First name"/>
                                        <Input id="_IDGenInput5" placeholder="Last name"/>
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                    </f:formContainers>
                </f:Form>
            </VBox>
            <VBox id="_IDGenVBox20" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle9" text="SAP.UI.LAYOUT.FORM with Responsive Grid Layout" class="titleBg" width="1230px" />
                <f:Form id="_IDGenForm2" editable="true" >
                    <f:title>
                        <core:Title id="_IDGenTitle31" text="Book Data"></core:Title>
                    </f:title>
                    <f:layout>
                        <f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
                            labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4"
                            emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                            singleContainerFullSize="false"/>
                    </f:layout>
                    <f:formContainers>
                        <f:FormContainer id="_IDGenFormContainer" title="General Information" >
                            <f:formElements>
                                <f:FormElement id="_IDGenFormElement3" label="Book name">
                                    <f:fields>
                                        <Input id="_IDGenInput6" placeholder="Book name"/>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement id="_IDGenFormElement4" label="ISBN">
                                    <f:fields>
                                        <Input id="_IDGenInput7" placeholder="ISBN"/>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement id="_IDGenFormElement5" label="Author">
                                    <f:fields>
                                        <Input id="_IDGenInput8" placeholder="First name">
                                            <layoutData>
                                                <l:GridData id="_IDGenGridData1" span="XL2 L2 M2 S12" />
                                            </layoutData>
                                        </Input>
                                        <Input id="_IDGenInput9" placeholder="Last name">
                                            <layoutData>
                                                <l:GridData id="_IDGenGridData2" span="XL2 L2 M2 S12" />
                                            </layoutData>
                                        </Input>
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                        <f:FormContainer id="_IDGenFormContainer2" title="Purchase Information" >
                            <f:formElements>
                                <f:FormElement id="_IDGenFormElement7" label="Price">
                                    <f:fields>
                                        <Input id="_IDGenInput10" placeholder="Price" type="Number" description="€"/>
                                    </f:fields>
                                </f:FormElement>
                                <f:FormElement id="_IDGenFormElement8" label="Purchasing Date">
                                    <f:fields>
                                        <DatePicker id="_IDGenDatePicker" />
                                    </f:fields>
                                </f:FormElement>
                            </f:formElements>
                        </f:FormContainer>
                    </f:formContainers>
                </f:Form>
            </VBox>
            <VBox id="_IDGenVBox21" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle10" text="SAP.UI.LAYOUT.SIMPLEFORM" class="titleBg" width="1230px" />
                <f:SimpleForm id="_IDGenSimpleForm" editable="true" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
                            labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4"
                            emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                            singleContainerFullSize="false" title="Book Data" >
                    <Title id="_IDGenTitle32" text="General Information"/>
                    <Label id="_IDGenLabel3" text="Book name" /><Input id="_IDGenInput12" placeholder="Book name" />
                    <Label id="_IDGenLabel4" text="ISBN"/><Input id="_IDGenInput13" placeholder="ISBN" />
                    <Label id="_IDGenLabel5" text="Author"/>
                    <Input id="_IDGenInput14" placeholder="First name">
                        <layoutData>
                            <l:GridData id="_IDGenGridData" span="XL2 L2 M2 S12" />
                        </layoutData>
                    </Input>
                    <Input id="_IDGenInput15" placeholder="Last name">
                        <layoutData>
                            <l:GridData id="_IDGenGridData3" span="XL2 L2 M2 S12" />
                        </layoutData>
                    </Input>
                    <core:Title id="_IDGenTitle33" text="Purchasing Information"/>
                    <Label id="_IDGenLabel6" text="Price"/>
                    <Input id="_IDGenInput16" placeholder="Price" type="Number" description="€"/>
                    <Label id="_IDGenLabel7" text="Purchasing Date"/>
                    <DatePicker id="_IDGenDatePicker1" />
                    
                </f:SimpleForm>
            </VBox>
            <VBox id="_IDGenVBox22" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle11" text="SAP.M.LIST" class="titleBg" width="1230px" />
                <List id="_IDGenList" items="{/books}" headerText="Books" >
                    <items>
                        <StandardListItem id="_IDGenStandardListItem" title="{name}" description="{author}"
                                          info="{= ${inStock} ? 'In Stock' : 'Not in Stock'}"
                                          infoState="{= ${inStock} ? 'Success' : 'Error'}"
                        />
                    </items>
                </List>
            </VBox>
            <VBox id="_IDGenVBox23" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle26" text="SAP.M.LIST Object List Item" class="titleBg" width="1230px" />
                <List id="_IDGenList2" items="{/books}" headerText="Books" >
                    <items>
                        <ObjectListItem id="_IDGenObjectListItem" title="{name}" number="{price}" numberUnit="{priceUnit}">
                            <firstStatus>
                                <ObjectStatus id="_IDGenObjectStatus" text="{= ${inStock} ? 'In Stock' : 'Not in Stock'}" state="{= ${inStock} ? 'Success' : 'Error'}"/>
                            </firstStatus>
                            <ObjectAttribute id="_IDGenObjectAttribute" title="Author" text="{auther}"/>
                            <ObjectAttribute id="_IDGenObjectAttribute1" title="Release Year" text="{releaseYear}"/>
                        </ObjectListItem>
                    </items>
                </List>
            </VBox>
            <VBox id="_IDGenVBox24" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle27" text="SAP.M.BUTTON" class="titleBg" width="1230px" />
            </VBox>
            <VBox id="_IDGenVBox25" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle28" text="SAP.M.BUTTON" class="titleBg" width="1230px" />
            </VBox>
            <VBox id="_IDGenVBox26" class="sapUiMediumMarginBottom myBorderBox" >
                <Title id="_IDGenTitle29" text="SAP.M.BUTTON" class="titleBg" width="1230px" />
            </VBox>
        </content>

        <footer>
            <OverflowToolbar id="footerToolbar2">

            </OverflowToolbar>
        </footer>

    </Page>

</mvc:View>